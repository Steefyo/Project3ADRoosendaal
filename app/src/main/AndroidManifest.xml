<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onboarding">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controller.VideoPagina.VideoPagina"></activity>
        <activity android:name=".Controller.Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.FeedbackPagina.FeedbackActivity" />
        <activity android:name=".Controller.InfoPagina.InfoActivity" />
        <activity android:name=".Controller.MenuPagina.MenuActivity" />
        <activity android:name=".Controller.SocialPagina.SocialActivity" />
        <activity android:name=".Controller.PersonalPagina.PersonalActivity" />
        <activity android:name=".Controller.WelcomePagina.WelcomeActivity" />
    </application>

</manifest>